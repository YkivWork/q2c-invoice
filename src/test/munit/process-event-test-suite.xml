    <munit:test name="processeventflow-test-suite-processEvent901FlowTest" description="Test" ignore="true">
        <mock:when messageProcessor="vm:outbound-endpoint" doc:name="Mock VM">
            <mock:then-return payload="#[samePayload()]"/>
        </mock:when>

        <set-payload value="#[getResource('examples/event-901-example.xml').asString()]" doc:name="Set Payload" mimeType="application/xml"/>
        <flow-ref name="processEventFlow" doc:name="processEventFlow"/>

        <munit:assert-on-equals message="failed on event id" expectedValue="#[&quot;901&quot;]" actualValue="#[flowVars.eventId]" doc:name="Assert Equals"/>
    </munit:test>
